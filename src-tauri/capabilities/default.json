{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "updater:default",
    "clipboard-manager:default",
    "dialog:allow-open",
    "dialog:allow-message",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive"
  ]
}